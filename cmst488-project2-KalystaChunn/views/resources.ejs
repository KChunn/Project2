<h1>Resources</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Provider</th>
    </tr>
  </thead>
  <tbody>
    <!-- Start resources.forEach loop -->
    <% resources.forEach(resource => { %>
      <tr>
        <td><%= resource.name %></td>
        <td><%= resource.provider %></td>
      </tr>
    <% }); %>
    <!-- End resources.forEach loop -->
  </tbody>
</table>